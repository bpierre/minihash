{
  "name": "minihash",
  "description": "A simple wrapper around window.location.hash",
  "version": "0.0.3",
  "author": "Pierre Bertet <bonjour@pierrebertet.net> (http://pierrebertet.net/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bpierre/minihash.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "browserify test/*.js | testling",
    "cover": "browserify -t coverify test/*.js | testling | coverify",
    "example": "browserify example/index.js -o example/bundle.js"
  },
  "dependencies": {},
  "devDependencies": {
    "browserify": "^3.32.0",
    "coverify": "^1.0.6",
    "tape": "^4.0.0",
    "testling": "^1.6.0"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/9..latest",
      "chrome/25..latest",
      "chrome/canary",
      "firefox/20..latest",
      "firefox/nightly",
      "safari/6..latest",
      "opera/11.0..latest",
      "opera/next",
      "iphone/6..latest",
      "ipad/6..latest",
      "android-browser/4.2..latest"
    ]
  }
}
